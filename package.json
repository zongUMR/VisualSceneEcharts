  },
  "homepage": "https://github.com/zongUMR/VisualSceneEcharts",
  "devDependencies": {
    "@babel/parser": "^7.10.1",
    "@kadira/storybook": "^1.19.0",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
